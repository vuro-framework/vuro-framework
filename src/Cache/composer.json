{
    "name": "vuro-framework/cache",
    "description": "A psr-6 and psr-16 compliant cache handler.",
    "keywords": [],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vuro-framework/vuro-framework/issues",
        "source": "https://github.com/vuro-framework/vuro-framework"
    },
    "require": {
        "php": ">=8.0",
        "psr/cache": "^3.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Vuro\\Cache\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
